{
  "version": "5.2.21",
  "name": "bellajs",
  "description": "A useful helper for any javascript program",
  "homepage": "https://www.npmjs.com/package/bellajs",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndaidong/bellajs"
  },
  "author": "@ndaidong",
  "main": "./index.js",
  "engines": {
    "node": ">= 6.0"
  },
  "scripts": {
    "test": "./node_modules/.bin/snyk test && ./node_modules/.bin/tape test/start.js | tap-spec",
    "coverage": "./node_modules/.bin/nyc tape test/start.js | tap-spec",
    "report": "npm run coverage && ./node_modules/.bin/nyc report --reporter=lcov",
    "coveralls": "npm run report && cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "build": "./node_modules/.bin/gccmin src dist/bella.min.js",
    "snyk": "./node_modules/.bin/snyk wizard"
  },
  "devDependencies": {
    "chance": "latest",
    "cheerio": "latest",
    "colors": "latest",
    "coveralls": "latest",
    "gcc-min": "latest",
    "is": "latest",
    "nyc": "latest",
    "sinon": "latest",
    "snyk": "latest",
    "tap-spec": "latest",
    "tape": "latest"
  },
  "keywords": [
    "detection",
    "manipulation",
    "templating",
    "datetime",
    "scheduler",
    "util"
  ],
  "license": "MIT",
  "maintainers": [
    "ndaidong <ndaidong@gmail.com>"
  ],
  "bugs": {
    "url": "https://github.com/ndaidong/bellajs/issues"
  },
  "directories": {
    "test": "test"
  }
}
