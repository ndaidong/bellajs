{
  "version": "6.4.2",
  "name": "bellajs",
  "description": "A useful helper for any javascript program",
  "homepage": "https://www.npmjs.com/package/bellajs",
  "repository": {
    "type": "git",
    "url": "https://github.com/ndaidong/bellajs"
  },
  "author": "@ndaidong",
  "main": "./index.js",
  "engines": {
    "node": ">= 6.0"
  },
  "scripts": {
    "lint": "eslint .",
    "snyk": "snyk test",
    "tape": "nyc tape test/start.js | tap-spec",
    "report": "nyc report --reporter=lcov",
    "pretest": "npm run lint && npm run snyk",
    "test": "npm run tape",
    "posttest": "npm run report",
    "coveralls": "npm test && cat ./coverage/lcov.info | coveralls",
    "build": "gccmin"
  },
  "devDependencies": {
    "chance": "latest",
    "cheerio": "latest",
    "colors": "latest",
    "coveralls": "latest",
    "eslint": "latest",
    "eslint-config-ggc": "latest",
    "gcc-min": "latest",
    "is": "latest",
    "nyc": "latest",
    "sinon": "latest",
    "snyk": "latest",
    "tap-spec": "latest",
    "tape": "latest"
  },
  "keywords": [
    "detection",
    "manipulation",
    "templating",
    "stabilize",
    "immutable",
    "datetime",
    "util"
  ],
  "gccmin": {
    "source": "src",
    "target": "dist",
    "filename": "bella",
    "globalVar": "Bella"
  },
  "license": "MIT"
}
